{
  "name": "rethinking/eloquent-sortable-relations",
  "description": "Sortable behaviour for eloquent hasMany relations",
  "keywords": [
    "sort",
    "sortable",
    "sorted",
    "eloquent",
    "model",
    "relation",
    "behaviour",
    "hasMany"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "laravel/framework": "~5.8.0|^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "~3.8.0|^4.0|^5.0",
    "squizlabs/php_codesniffer": "3.5.5",
    "phpmd/phpmd": "2.8.2"
  },
  "autoload": {
    "psr-4": {
      "Rethinking\\Eloquent\\Relations\\Sortable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rethinking\\Eloquent\\Relations\\Sortable\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "style": [
      "./vendor/bin/phpcs ./src/ --standard=PSR2",
      "./vendor/bin/phpmd ./src/ text codesize,unusedcode"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
